TRACER_ENABLE_METHOD=true
TRACER_ENABLE_EXCEPTION=true
ZIPKIN_ENDPOINT_URL=http://zipkin:9411/api/v2/spans

NACOS_HOST=nacos
NACOS_PORT=8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos
NACOS_GROUPNAME='DEFAULT_GROUP'
NACOS_NAMESPACE_ID='rpc-demo'
